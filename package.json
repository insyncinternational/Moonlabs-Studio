{
  "name": "moonlabs-studio",
  "version": "1.0.0",
  "description": "Moonlabs Studio - AI Venture Builder Website",
  "main": "index.html",
  "scripts": {
    "start": "python -m http.server 8000",
    "build": "npm run minify && npm run optimize",
    "minify": "npm run minify-css && npm run minify-js && npm run minify-html",
    "minify-css": "cleancss -o dist/styles.min.css styles.css",
    "minify-js": "terser script.js -o dist/script.min.js -c -m",
    "minify-html": "html-minifier-terser --input-dir . --output-dir dist --file-ext html --minify-js --minify-css --remove-comments --collapse-whitespace",
    "optimize": "npm run optimize-images",
    "optimize-images": "echo 'Image optimization would go here'",
    "deploy": "npm run build && npm run deploy-to-vercel",
    "deploy-to-vercel": "vercel --prod",
    "serve": "serve dist",
    "dev": "live-server --port=3000 --open=/index.html"
  },
  "keywords": [
    "ai",
    "venture-builder",
    "startup",
    "dubai",
    "moonlabs"
  ],
  "author": "Moonlabs Studio",
  "license": "MIT",
  "devDependencies": {
    "clean-css-cli": "^5.6.2",
    "terser": "^5.19.2",
    "html-minifier-terser": "^7.2.0",
    "live-server": "^1.2.2",
    "serve": "^14.2.1",
    "vercel": "^32.0.0"
  },
  "dependencies": {
    "font-awesome": "^6.0.0"
  }
}
